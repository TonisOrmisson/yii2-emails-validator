{
  "name": "andmemasin/yii2-emails-validator",
  "type": "project",
  "license":"MIT",
  "description": "A Yii2 module to do on-screen bulk validation of e-mails",
  "keywords": ["validation","yii2","email"],

  "authors": [
    {
      "name": "TÃµnis Ormisson",
      "email": "tonis@andmemasin.eu"
    }
  ],

  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0.2",
    "ext-intl": "*",
    "yiisoft/yii2": "~2.0.5",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "egulias/email-validator":"^3.2",
    "tonisormisson/yii-usuario-views-bootstrap4": "^1.0"
  },
  "require-dev": {
    "andmemasin/yii2-myabstract": "^4.0",
    "codeception/codeception": "^4.1",
    "codeception/specify": "~0.4.6",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "codeception/module-filesystem": "^1.0.0",
    "codeception/module-yii2": "^1.1.0",
    "codeception/module-asserts": "^1.3.0",
    "codeception/module-db": "^1.0",
    "phpstan/phpstan": "^1.8"

  },
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },

  "autoload": {
    "psr-4": {
      "andmemasin\\emailsvalidator\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
