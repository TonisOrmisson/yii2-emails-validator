{
  "name": "andmemasin/yii2-emails-validator",
  "type": "project",
  "license":"MIT",
  "description": "A Yii2 module to do on-screen bulk validation of e-mails",
  "keywords": ["validation","yii2","email"],

  "authors": [
    {
      "name": "TÃµnis Ormisson",
      "email": "tonis@andmemasin.eu"
    }
  ],

  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0.2",
    "ext-intl": "*",
    "yiisoft/yii2": "~2.0.5",
    "yiisoft/yii2-bootstrap5": "^2.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "egulias/email-validator": "^3|^4"
  },
  "require-dev": {
    "andmemasin/yii2-myabstract": "^5.0",
    "codeception/codeception": "^5.0",
    "codeception/module-filesystem": "^3.0.0",
    "codeception/module-yii2": "^1.1.7",
    "codeception/module-asserts": "^3.0.0",
    "codeception/module-db": "^3.0",
    "phpstan/phpstan": "^1.8"
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },

  "autoload": {
    "psr-4": {
      "andmemasin\\emailsvalidator\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
